config_version=4

[application]

config/name="Picross"
config/description="A picross game that dynamically loads the level from a black and white image."
run/main_scene="res://nodes/Menus.tscn"
config/icon="res://icon.png"
config/windows_native_icon="res://favicon.ico"

[autoload]

Game="*res://global/game.gd"
Levels="*res://global/levels.gd"
Save="*res://global/save.gd"
AudioPlayer="*res://global/Audio Player.tscn"
Globals="*res://global/globals.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray(  )

